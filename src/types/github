export interface GitHubRepo {
  id: number;
  node_id: string;
  name: string;
  full_name: string;
  private: boolean;
  owner: {
    login: string;
    id: number;
    node_id: string;
    avatar_url: string;
    // ... other owner properties
  };
  html_url: string;
  description: string | null;
  fork: boolean;
  url: string;
  // ... other repository properties
}